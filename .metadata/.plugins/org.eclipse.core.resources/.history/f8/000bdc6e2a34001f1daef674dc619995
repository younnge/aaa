package ex01;

public class ex03 {

	public static void main(String[] args) {
		System.out.println("10+3 = " + (10 + 3));
		System.out.println("50/3 = " + 50 / 3);
		System.out.println("50% = " + 50 % 3);
		System.out.println(3 * 4);
		System.out.println(10 - 7);
	}
}
//몫은 % 표시
//main ctrl space
//sysout
//값 계산 f11
